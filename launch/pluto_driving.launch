<launch>

   <node name="soundplay_node" pkg="sound_play" type="soundplay_node.py" output="screen"/>


   <node name="driving" pkg="ros_driving" type="driving_node.py">
  	<param name="voice" value="voice_rab_diphone"/>
  	<param name="wavepath" value="$(find ros_driving)/sounds"/>
   </node>
</launch>
